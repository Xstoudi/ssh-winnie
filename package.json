{
  "name": "ssh-winnie",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch --encore-args=\"--port 3555\"",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "preload": "node -e \"import('geolite2-redist').then(geolite => geolite.downloadDbs())\""
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@babel/preset-react": "^7.18.6",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@symfony/webpack-encore": "^3.0.0",
    "@tailwindcss/forms": "^0.5.2",
    "@types/d3": "^6",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.2",
    "@types/pg": "^8.6.5",
    "@types/ssh2": "^1.11.5",
    "@types/topojson": "^3.2.3",
    "@types/webpack": "^5.28.0",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "pino-pretty": "^8.1.0",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.5",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/view": "^6.1.6",
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "csv-stringify": "^6.2.0",
    "d3": "^6.7.0",
    "d3-geo": "^3.0.1",
    "d3-selection": "^2.0.0",
    "geolite2-redist": "^3.0.2",
    "i18n-iso-countries": "^7.5.0",
    "knex": "^2.2.0",
    "lodash.startcase": "^4.4.0",
    "luxon": "^3.0.1",
    "maxmind": "^4.3.6",
    "pg": "^8.7.3",
    "pg-query-stream": "^4.2.3",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.13",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "ssh-keygen-lite": "^1.3.0",
    "ssh2": "^1.11.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1"
  }
}
